{
  "name": "@cactus-technologies/server",
  "version": "1.1.7",
  "description": "Basic Server for Cactus Webservices",
  "homepage": "https://github.com/CactusTechnologies/cactus-bare-server#readme",
  "bugs": {
    "url": "https://github.com/CactusTechnologies/cactus-bare-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CactusTechnologies/cactus-bare-server.git"
  },
  "license": "UNLICENCED",
  "author": "Cactus Technologies, LLC <hi@cactus.is> (https://www.cactus.is)",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "precommit": "npm test && npm run docs && lint-staged",
    "docs": "run-s docs:* -sn  && git add {README,TODO}.md AUTHORS",
    "docs:authors": "git log --format='%aN <%aE>' | sort -f | uniq > AUTHORS",
    "docs:readme": "mos README.md",
    "docs:todos": "npm run todos -s -- --reporter markdown > TODO.md",
    "test": "run-s test:* -sn",
    "test:lint": "eslint './**/*.js'",
    "test:outdated": "npm outdated --long || true",
    "todos": "leasot './**/**' --skip-unsupported --ignore './node_modules/**/**' --exit-nicely"
  },
  "dependencies": {
    "@cactus-technologies/node-application": "^3.0.4",
    "@cactus-technologies/uuid": "^1.0.3",
    "@cactus-technologies/errors": "^1.2.0",
    "@cactus-technologies/logger": "^2.1.0",
    "@cactus-technologies/utils": "^1.0.8",
    "compression": "^1.7.2",
    "config": "^2.0.1",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "ipaddr.js": "^1.7.0",
    "lodash": "^4.17.10",
    "on-headers": "^1.0.1",
    "serve-favicon": "^2.5.0",
    "sort-object-keys": "^1.1.2"
  },
  "devDependencies": {
    "depcheck": "^0.6.9",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "leasot": "^6.3.0",
    "lint-staged": "^7.2.0",
    "mos": "^1.3.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7"
  }
}
