{
  "name": "@cactus-technologies/errors",
  "version": "1.2.0",
  "description": "Errors module for Cactus Servers and Applications",
  "homepage": "https://github.com/CactusTechnologies/cactus-server-errors#readme",
  "bugs": {
    "url": "https://github.com/CactusTechnologies/cactus-server-errors/issues"
  },
  "license": "UNLICENCED",
  "author": "Cactus Technologies, LLC <hi@cactus.is> (https://www.cactus.is)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CactusTechnologies/cactus-server-errors.git"
  },
  "scripts": {
    "precommit": "npm test && npm run docs && lint-staged",
    "docs": "run-s docs:* -sn  && git add {README,TODO}.md AUTHORS",
    "docs:authors": "git log --format='%aN <%aE>' | sort -f | uniq > AUTHORS",
    "docs:readme": "mos README.md",
    "docs:todos": "npm run todos -s -- --reporter markdown > TODO.md || true",
    "test": "run-s test:* -sn",
    "test:depcheck": "depcheck --ignores=husky,prettier",
    "test:lint": "standard --verbose | snazzy",
    "todos": "leasot './**/**' --skip-unsupported --ignore './node_modules/**/**'"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "depcheck": "^0.6.8",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "leasot": "^4.13.0",
    "lint-staged": "^7.0.0",
    "mos": "^1.3.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "snazzy": "^7.0.0",
    "standard": "^11.0.0"
  }
}
