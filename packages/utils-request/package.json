{
  "name": "@cactus-technologies/api-request",
  "version": "1.2.2",
  "description": "Request helper",
  "keywords": [],
  "homepage": "https://github.com/CactusTechnologies/cactus-request#readme",
  "bugs": {
    "url": "https://github.com/CactusTechnologies/cactus-request/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CactusTechnologies/cactus-request.git"
  },
  "license": "UNLICENCED",
  "author": "Cactus Technologies, LLC <hi@cactus.is> (https://www.cactus.is)",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run docs && lint-staged",
    "docs": "run-s docs:* -sn",
    "docs:authors": "git log --format='%aN <%aE>' | sort -f | uniq > AUTHORS && git add AUTHORS",
    "docs:readme": "mos README.md && git add README.md",
    "docs:todos": "npm run todos -s -- --reporter markdown > TODO.md && git add TODO.md",
    "prepush": "npm run test",
    "test": "run-s test:* -sn",
    "test:depcheck": "depcheck --ignores=husky --ignore-bin-package",
    "test:lint": "eslint './**/*.js'",
    "test:outdated": "npm outdated --long || true",
    "todos": "leasot -xS --ignore './node_modules/**/**' './**/**'"
  },
  "dependencies": {
    "bottleneck": "^2.5.0",
    "lodash": "^4.17.5",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "depcheck": "^0.6.8",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "leasot": "^6.3.0",
    "lint-staged": "^7.2.0",
    "mos": "^1.3.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.13.7"
  },
  "peerDependencies": {
    "@cactus-technologies/errors": "^1.2.0",
    "@cactus-technologies/logger": "^2.0.1"
  }
}
