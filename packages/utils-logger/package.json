{
  "name": "@cactus-technologies/lab100-logger",
  "version": "1.2.0",
  "description": "Customized Logger for the Lab100 ecosystem",
  "homepage": "https://github.com/CactusTechnologies/INGH_logger#readme",
  "bugs": {
    "url": "https://github.com/CactusTechnologies/INGH_logger/issues"
  },
  "license": "UNLICENCED",
  "author": "Cactus Technologies, LLC <hi@cactus.is> (https://www.cactus.is)",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CactusTechnologies/INGH_logger.git"
  },
  "scripts": {
    "precommit": "npm test && npm run docs && lint-staged",
    "docs": "run-s docs:* -sn  && git add {README,TODO}.md AUTHORS",
    "docs:authors": "git log --format='%aN <%aE>' | sort -f | uniq > AUTHORS",
    "docs:readme": "mos README.md",
    "docs:todos": "npm run todos -s -- --reporter markdown > TODO.md",
    "test": "run-s test:* -sn",
    "test:depcheck": "depcheck --ignores=husky,prettier",
    "test:lint": "standard --verbose | snazzy",
    "test:outdated": "npm outdated --long",
    "todos": "leasot './**/**' --skip-unsupported --ignore './node_modules/**/**' --ignore './**/*.min.*' --exit-nicely"
  },
  "dependencies": {
    "@mechanicalhuman/bunyan-pretty": "^1.1.4",
    "bunyan": "^1.8.12",
    "bunyan-cloudwatch": "^2.2.0",
    "config": "^1.30.0",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "pretty-bytes": "^4.0.2",
    "pretty-ms": "^3.1.0",
    "safe-json-stringify": "^1.1.0",
    "sort-object-keys": "^1.1.2"
  },
  "devDependencies": {
    "depcheck": "^0.6.8",
    "husky": "^0.14.3",
    "leasot": "^6.1.0",
    "lint-staged": "^7.0.2",
    "mos": "^1.3.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "snazzy": "^7.0.0",
    "standard": "^11.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
