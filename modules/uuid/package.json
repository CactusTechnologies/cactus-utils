{
  "name": "@cactus-technologies/uuid",
  "version": "1.0.5-0",
  "description": "A set of Unique ID generators for JS apps",
  "homepage": "https://github.com/CactusTechnologies/cactus-uuid#readme",
  "bugs": {
    "url": "https://github.com/CactusTechnologies/cactus-uuid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CactusTechnologies/cactus-uuid.git"
  },
  "license": "MIT",
  "author": "Cactus Technologies LLC <hi@cactus.is> (http://www.cactus.is)",
  "contributors": [
    "Jorge Proa√±o <jorge@hiddennodeproblem.com> (http://www.hidden-node-problem.com)"
  ],
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "docs": "run-s docs:** -sn",
    "docs:01:authors": "git log --format='%aN <%aE>' | sort -f | uniq > AUTHORS",
    "docs:02:readme": "jsdoc2md --template .readme.hbs index.js > README.md",
    "docs:03:todos": "npm run todos -s -- --reporter vscode >> README.md",
    "docs:04:update": "projectz compile",
    "docs:05:add": "git add README.md AUTHORS LICENSE package.json",
    "test": "run-s test:* -sn",
    "test:depcheck": "depcheck --ignores=husky --ignore-bin-package",
    "test:lint": "eslint --ignore-path .gitignore '**/*.js'",
    "test:outdated": "npm outdated --long || true",
    "todos": "leasot './**/*' -Sx -i './node_modules/**/*'"
  },
  "dependencies": {
    "random-js": "^1.0.8"
  },
  "devDependencies": {
    "depcheck": "^0.6.9",
    "eslint": "^5.2.0",
    "eslint-config-standard": "12.0.0-alpha.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jsdoc-to-markdown": "^4.0.1",
    "leasot": "^6.3.0",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0",
    "projectz": "^1.4.0",
    "sort-package-json": "^1.15.0"
  },
  "maintainers": [],
  "private": true
}
