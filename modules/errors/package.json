{
  "name": "@cactus-technologies/errors",
  "version": "1.2.1-0",
  "description": "Errors module for Cactus Servers and Applications",
  "homepage": "https://github.com/CactusTechnologies/cactus-server-errors#readme",
  "bugs": {
    "url": "https://github.com/CactusTechnologies/cactus-server-errors/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CactusTechnologies/cactus-server-errors.git"
  },
  "license": "UNLICENCED",
  "author": "Cactus Technologies, LLC <hi@cactus.is> (https://www.cactus.is)",
  "main": "index.js",
  "scripts": {
    "docs": "run-s docs:* -sn  && git add {README,TODO}.md AUTHORS",
    "docs:authors": "git log --format='%aN <%aE>' | sort -f | uniq > AUTHORS",
    "docs:readme": "mos README.md",
    "docs:todos": "npm run todos -s -- --reporter markdown > TODO.md",
    "test": "run-s test:* -sn",
    "test:depcheck": "depcheck --ignores=husky --ignore-bin-package",
    "test:lint": "eslint --ignore-path .gitignore '**/*.js'",
    "test:outdated": "npm outdated --long || true",
    "todos": "leasot './**/*' -Sx -i './node_modules/**/*'"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "depcheck": "^0.6.9",
    "eslint": "^5.2.0",
    "eslint-config-standard": "12.0.0-alpha.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "leasot": "^6.3.0",
    "lint-staged": "^7.2.0",
    "mos": "^1.3.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0"
  },
  "private": true
}
